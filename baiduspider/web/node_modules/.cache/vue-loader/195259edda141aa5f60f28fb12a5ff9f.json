nimport WebResult from '@/components/WebResult'\n\nexport default {\n  name: 'WebResults',\n  data: () => {\n    return {\n      query: '',\n      results: null,\n      resultsNormal: [],\n      resultId: 0\n    }\n  },\n  methods: {\n    searchWeb: function (query) {\n      if (!query) return\n      // eslint-disable-next-line\n      this.$router.push(`/search/web?q=${encodeURIComponent(query)}`).catch((err) => {\n        // eslint-disable-next-line\n        return\n      })\n    },\n    getResults: async function () {\n      await SearchService.searchWeb(this.query, 5).then((data) => {\n        this.resultsNormal = []\n        this.results = data.data.results\n        for (var i = 0; i < this.results.results.length; i++) {\n          if (this.results.results[i].type === 'result') {\n            this.resultsNormal.push(this.results.results[i])\n          }\n        }\n        this.resultId += 1\n      })\n    }\n  },\n  components: {\n    ResultsNavbar,\n    WebResult\n  },\n  created: function () {\n    this.query = this.$route.query.q\n    this.getResults()\n  },\n  watch: {\n    '$route.query.q': function () {\n      this.query = this.$route.query.q\n      this.getResults()\n    }\n  }\n}\n</script>\n\n<style scoped>\n@media screen and ( max-width: 400px ) {\n  .search {\n    width: 330px;\n  }\n}\n@media screen and ( min-width: 1000px ) {\n  .search {\n    width: 950px;\n  }\n}\n</style>\n"]}]}