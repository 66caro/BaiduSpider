 </v-container>\n    <v-container\n      class=\"fill-height\"\n      fluid\n    >\n      <v-row\n        align=\"center\"\n        justify=\"center\"\n        style=\"margin-bottom: 300px\"\n      >\n        <v-col\n          cols=\"12\"\n          sm=\"8\"\n          class=\"text-center\"\n        >\n          <h1>BaiduSpider</h1>\n          <br>\n          <v-divider/>\n          <br>\n          <v-form\n            @submit=\"searchWeb()\"\n            onSubmit=\"return false\"\n          >\n            <v-text-field\n              v-model=\"query\"\n              label=\"搜索\"\n              solo-inverted\n              rounded\n              append-outer-icon=\"mdi-magnify\"\n              clearable\n              clear-icon=\"mdi-close\"\n              autocomplete=\"off\"\n              @click:append-outer=\"searchWeb()\"\n            ></v-text-field>\n          </v-form>\n        </v-col>\n      </v-row>\n    </v-container>\n  </v-main>\n</template>\n\n<script>\nimport StatusService from '@/services/StatusService'\n\nexport default {\n  name: 'SearchBox',\n\n  data: () => {\n    return {\n      query: '',\n      tab: null,\n      items: [\n        '网页'\n      ],\n      offline: null\n    }\n  },\n  methods: {\n    searchWeb: function () {\n      if (!this.query) {\n        return\n      }\n      this.$router.push({\n        path: '/search/web',\n        query: {\n          q: this.query\n        }\n      })\n    },\n    getStatus: async function () {\n      let online\n      await StatusService.getStatus().then((data) => {\n        online = true\n      }).catch(() => {\n        online = false\n      })\n      this.offline = !online\n    }\n  },\n  created: function () {\n    this.getStatus()\n  }\n}\n</script>\n"]}]}